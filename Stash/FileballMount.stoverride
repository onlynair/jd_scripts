name: FileballMount
desc: 添加Synoogy协议，账号随便填，密码填cookie
# 脚本作者小白脸 获取ck的方法
# 阿里云Token获取地址(需要用阿里云盘扫描alist的二维码获得)[ https://alist-doc.nn.ci/docs/driver/aliyundrive ]
# 夸克登录网页版抓包，路径https://drive.quark.cn/1/clouddrive/file
# pikpak 直接填账号密码

http:

  script:

    - match: ^http:\/\/aliyun\.example\.com
      name: 阿里云挂载
      type: request
      timeout: 30
      require-body: true
      max-size: 3145728

    - match: ^http:\/\/quark\.example\.com
      name: 夸克挂载
      type: request
      timeout: 30
      require-body: true
      max-size: 3145728

    - match: ^http:\/\/pikpak\.example\.com
      name: pikpak挂载
      type: request
      timeout: 30
      require-body: true
      max-size: 3145728

script-providers:
  阿里云挂载:
    url: https://raw.githubusercontent.com/githubdulong/Script/master/ali.js
    interval: 86400
  夸克挂载:
    url: https://raw.githubusercontent.com/githubdulong/Script/master/quark.js
    interval: 86400
  pikpak挂载:
    url: https://raw.githubusercontent.com/githubdulong/Script/master/pikpak.js
    interval: 86400
